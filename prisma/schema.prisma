datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

model TD_ASP {
  OdPcPt_No     String    @db.VarChar(14)
  Process_No    String    @db.VarChar(2)
  PPC           String?   @db.VarChar(6)
  Parts_Qty     Int?      @db.Int
  M_Resource_CD String?   @db.VarChar(20)
  BS_Time       DateTime? @db.DateTime
  BE_Time       DateTime? @db.DateTime
  PS_Time       DateTime? @db.DateTime
  PE_Time       DateTime? @db.DateTime
  AS_Time       DateTime? @db.DateTime
  AE_Time       DateTime? @db.DateTime
  Result_CAT    String    @db.VarChar(1)
  OdPtPr_No     String    @db.VarChar(12)

  @@id([OdPcPt_No, Process_No])
}

model TD_Cost {
  Order_No              String       @db.VarChar(20) // หมายเลขคำสั่ง (Short Text)
  Parts_No              String       @db.VarChar(2) // หมายเลขชิ้นส่วน (Short Text)
  Cost_No               String       @db.VarChar(4) // หมายเลขค่าใช้จ่าย (Short Text)
  Process_No            String?      @db.VarChar(2) // หมายเลขกระบวนการ (Short Text)
  OdPt_No               String?      @db.VarChar(22) // หมายเลข OdPt (Short Text)
  OdPtCs_No             String?      @db.VarChar(26) // หมายเลข OdPtCs (Short Text)
  OdPtPr_No             String?      @db.VarChar(24) // หมายเลข OdPtPr (Short Text)
  CMC                   String?      @db.VarChar(8) // CMC (Short Text)
  CMT                   Int?         @db.SmallInt // CMT (Short Text)
  CPC                   String?      @db.VarChar(10)
  CPT                   Int?         @db.SmallInt // CPT (Short Text)
  CPD                   DateTime?    @db.DateTime()
  CPN                   Int?         @db.SmallInt
  Cs_Progress_CD        String?      @db.VarChar(2) // รหัสความคืบหน้า (Short Text)
  Cs_Complete_Date      DateTime?    @db.DateTime() // วันที่ทำเสร็จสมบูรณ์ (Date/Time)
  Cs_Complete_Qty       Int?         @db.SmallInt // จำนวนที่ทำเสร็จ (Number)
  Cs_Label_CSV          Boolean?     @db.Bit(1) // CSV (Yes/No)
  Cs_All_Complete       Boolean?     @db.Bit(1) // เสร็จทั้งหมด (Yes/No)
  Cs_Order_All_Complete Boolean?     @db.Bit(1) // คำสั่งทั้งหมดเสร็จสิ้น (Yes/No)
  Cs_Parts_Complete     Boolean?     @db.Bit(1) // ชิ้นส่วนเสร็จสิ้น (Yes/No)
  Cs_Final_Complete     Boolean?     @db.Bit(1) // เสร็จสมบูรณ์ทั้งหมด (Yes/No)
  Cs_Remark             String?      @db.VarChar(255) // ข้อสังเกต (Short Text)
  Cs_Register_Date      DateTime?    @db.DateTime() // วันที่ลงทะเบียน (Date/Time)
  Cs_Modify_Date        DateTime?    @db.DateTime() // วันที่แก้ไข (Date/Time)
  Cs_Reg_Person_CD      String?      @db.VarChar(10) // รหัสผู้ลงทะเบียน (Short Text)
  Cs_Upd_Person_CD      String?      @db.VarChar(10) // รหัสผู้ปรับปรุง (Short Text)
  Sequence_No           Int?         @db.Int
  // เชื่อมความสัมพันธ์กับ TD_Plan (Many-to-One)
  plan                  TD_Plan      @relation(fields: [Order_No, Parts_No], references: [Order_No, Parts_No], name: "PlanToCost")
  TD_Order              TD_Order?    @relation(fields: [Order_No], references: [Order_No]) // ความสัมพันธ์กับ TD_Order
  TM_Resource           TM_Resource? @relation(fields: [CMC], references: [Resource_CD])

  @@id([Order_No, Parts_No, Cost_No]) // กำหนดคีย์หลักร่วมกัน
}

